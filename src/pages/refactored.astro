					const regexLastUnderscore = /_(?=[^_]*$)/g;
					const regexLastDash = /-(?=[^-]*$)/g;
					const regexDashUnderscore = /\w+-\w+_\w+/;
					const regexText = /^[^-]*/;
					const regexGroupName = /^([A-Z]{2}\d{2}-\d{1,2})/;
					const regexUnitNumber = /(\d{1,3})\D*$/;

