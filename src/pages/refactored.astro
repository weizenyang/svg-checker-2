						const refEl = doc
							.querySelector("defs")
							.querySelector(refId);
						var child;
						if (refEl) {
							console.log(refEl);
							child = refEl.children[0];
							child.classList.remove(child.classList[0]);
							child.classList.add(e);
							units.push(child);
							return child;
						}
